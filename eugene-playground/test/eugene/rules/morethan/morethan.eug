PartType StataPart();

for(num i=1; i<=10; i++) {
    StataPart ** "stata-"+i ** ();
}


Device StataDevice(
	StataPart, StataPart, StataPart, StataPart, StataPart,
	StataPart, StataPart, StataPart, StataPart, StataPart	
);
	
/*
 * k=0 ... 10,000,000,000  (10 to the 10 => 10 billion)
 */	
product(StataDevice, 20000);	


/* 
 * k=1 ... 
 */
Rule r1(stata-1 MORETHAN 1);
product(StataDevice, strict, 2000);

/*
 *  the stata-1 part should appeare more than 9 times in the statadevice
 *  ie 1 device should be the result
 */
Rule r2(stata-1 MORETHAN 9);
product(StataDevice, strict);


