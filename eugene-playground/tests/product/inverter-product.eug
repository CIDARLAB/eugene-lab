PartType Promoter();
PartType RBS();
PartType Gene();
PartType Terminator();

num N = 100;
for(num i=1; i<=N; i++) {
	Promoter ** "promoter"+i ** ();
	RBS ** "rbs"+i ** ();
	Gene ** "gene"+i ** ();
	Terminator ** "terminator"+i ** ();
}

Device TU1 (Promoter, RBS, Gene, Terminator);
Device TU2 (Promoter, RBS, Gene, Terminator);
Device TU3 (Promoter, RBS, Gene, Terminator);
Device TU4 (Promoter, RBS, Gene, Terminator);

Device Inverter(TU1, TU2, TU3, TU4);


Rule r(ON Inverter:
    CONTAINS promoter1
    AND
    CONTAINS rbs1);
    
product(Inverter, strict);
